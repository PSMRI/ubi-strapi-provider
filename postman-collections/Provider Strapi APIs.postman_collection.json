							"raw": "{\n    \"data\": {\n        \"title\": \"Student Education Scholarship 2025\",\n        \"category\": \"Education\",\n        \"subCategory\": \"Higher Education\",\n        \"tags\": [\n            {\n                \"name\": \"student\"\n            },\n            {\n                \"name\": \"scholarship\"\n            }\n        ],\n        \"applicationOpenDate\": \"2025-01-01\",\n        \"applicationCloseDate\": \"2025-12-31\",\n        \"imageUrl\": \"https://example.com/scholarship-image.jpg\",\n        \"shortDescription\": \"Financial assistance for higher education students from economically disadvantaged backgrounds.\",\n        \"longDescription\": \"This comprehensive scholarship program provides financial support to meritorious students from economically disadvantaged backgrounds to pursue higher education.\",\n        \"benefits\": [\n            {\n                \"__component\": \"benefit.financial-benefit\",\n                \"title\": \"Tuition Fee Coverage\",\n                \"type\": \"financial\",\n                \"description\": \"Full or partial coverage of tuition fees based on eligibility criteria\",\n                \"minValue\": \"10000\",\n                \"maxValue\": \"100000\"\n            }\n        ],\n        \"exclusions\": [\n            {\n                \"description\": \"Students with family income above Rs. 5 lakhs per annum are not eligible\"\n            }\n        ],\n        \"references\": [\n            {\n                \"title\": \"Official Guidelines\",\n                \"link\": \"https://education.gov.in/scholarship-guidelines\"\n            }\n        ],\n        \"providingEntity\": {\n            \"name\": \"Ministry of Education\",\n            \"type\": \"government\",\n            \"address\": {\n                \"street\": \"Shastri Bhawan, A Wing\",\n                \"city\": \"New Delhi\",\n                \"state\": \"Delhi\",\n                \"postalCode\": \"110001\"\n            },\n            \"department\": \"Department of Higher Education\",\n            \"contactInfo\": {\n                \"phoneNumber\": \"+91-11-23383936\",\n                \"email\": \"scholarship@education.gov.in\"\n            }\n        },\n        \"sponsoringEntities\": [\n            {\n                \"name\": \"Government of India\",\n                \"type\": \"government\",\n                \"address\": {\n                    \"street\": \"South Block, Raisina Hill\",\n                    \"city\": \"New Delhi\",\n                    \"state\": \"Delhi\",\n                    \"postalCode\": \"110001\"\n                },\n                \"department\": \"Ministry of Education\",\n                \"contactInfo\": {\n                    \"phoneNumber\": \"+91-11-23012312\",\n                    \"email\": \"info@education.gov.in\"\n                },\n                \"sponsorShare\": \"100%\"\n            }\n        ],\n        \"eligibility\": [\n            {\n                \"evidence\": \"Income Certificate\",\n                \"type\": \"economic\",\n                \"description\": \"Family income should be below Rs. 5 lakhs per annum\",\n                \"criteria\": {\n                    \"name\": \"annual_income\",\n                    \"condition\": \"less than equals\",\n                    \"conditionValues\": [\n                        500000\n                    ]\n                },\n                \"allowedProofs\": [\n                    \"income_certificate\",\n                    \"salary_slip\",\n                    \"bank_statement\"\n                ]\n            },\n            {\n                \"evidence\": \"Academic Performance\",\n                \"type\": \"academic\",\n                \"description\": \"Minimum 80% marks in previous qualifying examination\",\n                \"criteria\": {\n                    \"name\": \"percentage\",\n                    \"condition\": \"greater than equals\",\n                   